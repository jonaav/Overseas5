<!-- Modal -->
<div class="modal fade" id="formFeedbackModal" tabindex="-1" role="dialog" aria-labelledby="feedbackModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="feedbackModalLabel">Ingrese algun comentario</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group form-check-inline">
                        <label class="col">Alumno:</label>
                        <h5 id="txtAlumnoFeedback"></h5>
                    </div>
                    <div class="form-group">
                        <label class="col">Feedback:</label>
                        <textarea id="txtFeedback" rows="3" class="form-control"></textarea>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button onclick="GuardarFeedback()" class="btn btn-info" data-dismiss="modal">Guardar</button>
            </div>
        </div>
    </div>
</div>