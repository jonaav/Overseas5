                                <input type="text" id="txtIdCurso" hidden>                                                                                                                  
                            </div>                              
                        </div>                                                              
					</div>
                    <div class="form-group">
                        <div class="form-check">
                            <input class="form-check-input" type="checkbox" value="habilitar" id="checkBoxDocente" name="checkBoxDocente">
                            <label class="form-check-label" for="checkBoxDocente">
                             Docente Sin Asignar
                            </label>
                        </div>
                    </div>
                    <div class="form-group" >
					    <div class="col text-center" >
                            <div id="divGuardarTraduccion">
                                <button onclick='GuardarEditarTraduccion("Guardar")' rel="tooltip" title="Guardar Traduccion" type="button" class="btn btn-outline-success"><span class="fa fa-save" style="color:black"></button>
                                <button onclick='CargarTablaListadoTraduccion()' rel="tooltip" title="Cancelar"  type="button" class="btn btn-outline-danger"><span class="fa fa-remove" style="color:black"></button>
					        </div>
                            <div id="divEditarTraduccion">
                                <button onclick='GuardarEditarTraduccion("Editar")' rel="tooltip" title="Editar Traduccion" type="button" class="btn btn-outline-warning"><span class="fa fa-save" style="color:black"></button>
